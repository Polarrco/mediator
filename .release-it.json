  "git": {
    "commitMessage": "chore(release): release v${version}",
    "commit": false,
    "tag": true,
    "push": true
  },
  "github": {
    "release": true
  },
  "plugins": {
    "@release-it/conventional-changelog": {
      "preset": "angular",
      "infile": "CHANGELOG.md"
    }
  }
}
